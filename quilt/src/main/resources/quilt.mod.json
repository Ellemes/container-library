{
  "schema_version": 1,
  "quilt_loader": {
    "group": "ninjaphenix",
    "id": "ninjaphenix_container_lib",
    "version": "${version}",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": "ninjaphenix.container_library.quilt.Main",
      "rei": "ninjaphenix.container_library.quilt.client.ReiCompat",
      "modmenu": "ninjaphenix.container_library.quilt.client.ModMenuCompat"
    },
    "depends": [
      {
        "id": "org.quiltmc:quilt_loader",
        "versions": "~0.16.0-beta.12"
      },
      {
        "id": "minecraft",
        "versions": "~1.18.2"
      },
      "fabric-networking-api-v1",
      "fabric-screen-handler-api-v1",
      "fabric-key-binding-api-v1",
      "quilt_base"
    ],
    "metadata": {
      "name": "NinjaPhenix's Container Library",
      "description": "A library mod which provides an easy way for modders to display simple inventories to the user.",
      "contributors": {
        "NinjaPhenix": "Owner"
      },
      "contact": {
        "homepage": "https://modrinth.com/mod/ninjaphenix-container-library/",
        "sources": "https://github.com/NinjaPhenix/container-library/",
        "issues": "https://github.com/NinjaPhenix/container-library/issues/"
      },
      "license": [
        "0BSD"
      ],
      "icon": "icon.png"
    }
  },
  "mixin": "ninjaphenix-container-library.mixins.json"
}
